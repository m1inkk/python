a=int(input("Введіть а: "))

while a<0:
    a=int(input("a не може бути від'ємним. Введіть ще раз: "))

b=int(input("Введіть b: "))
while b<0:
    b=int(input("b не може бути від'ємним. Введіть ще раз: "))

if a>b:
    x=b * a + 1

elif a==b:
    x= -10

else:
    x= (a-5)/b

print("Результат: х= ", x)
